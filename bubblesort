#include<stdio.h>

void bubbleSort(int *v, int N){
    int i, aux;
    
    for(i = 0;i < N; i++){
        
        for(j = 0; j < N; j++){
            
            if (V[i] > V[j]){
                aux = V[j];
                V[i] = V[j];
                V[j] = aux;
            }
            
        }    
        
        
    }
                
}


